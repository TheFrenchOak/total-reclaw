{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "typeRoots": ["./node_modules/@types"],
    "paths": {
      "openclaw/plugin-sdk": ["./types/openclaw/plugin-sdk"]
    }
  },
  "include": ["*.ts", "src/**/*.ts", "types/**/*.d.ts", "tests/**/*.ts"]
}
